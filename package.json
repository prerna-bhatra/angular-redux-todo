    "@ngrx/store": "^13.1.0",
    "@ngrx/store-devtools": "^13.1.0",
    "rxjs": "~7.5.5",
    "todomvc-app-css": "~2.4.2",
    "uuid": "~8.3.2",
    "zone.js": "~0.11.4"
